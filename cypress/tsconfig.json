{
  "compilerOptions": {
    "typeRoots": ["../types", "../node_modules/@types", "./types"],
    "target": "es5",
    "lib": ["es5", "dom"],
    "types": ["cypress", "node", "cypress-axe"],
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@lib/*": ["lib/*"],
      "@components/*": ["components/*"],
      "@appComponents/*": ["app/_components/*"],
      "@formbuilder/*": ["components/form-builder/*"],
      "@pages/*": ["pages/*"],
      "@app/*": ["app/*"],
      "@jestFixtures/*": ["__fixtures__/*"],
      "@jestUtils": ["__utils__"],
      "@i18n": ["app/i18n"],
      "@i18n/*": ["app/i18n/*"],
      "@content/*": ["public/static/content/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "**/*.jsx",
    "../**/*.cy.tsx",
    "../**/*.cy.ts",
    "../cypress.config.ts",
    "support/commands/selection-command.js"
  ]
}
