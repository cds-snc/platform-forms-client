{
  "$schema": "http://json-schema.org/draft-07/schema",
  "id": "https://forms-formulaires.alpha.canada.ca/",
  "title": "GC Forms",
  "description": "An array of submission names and a description of the problem",
  "type": "object",
  "properties": {
    "entries": {
      "type": "array",
      "items": {
        "type": "string",
        "pattern": "^[0-9]{2}-[0-9]{2}-[a-z0-9]{4}$"
      },
      "minItems": 1
    },
    "description": {
      "type": "string"
    }
  },
  "required": ["entries", "description"],
  "additionalProperties": false
}
