  "name": "gcforms",
  "version": "1.0.0",
  "private": true,
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seeds/seed.ts --environment=development"
  },
  "scripts": {
    "dev": "yarn migrate:dev && yarn initialize && NODE_OPTIONS='--inspect' next dev",
    "build": "DATABASE_URL=postgres://postgres:password@localhost:5432 next build",
    "start": "yarn migrate:prod && yarn initialize && next start -p ${PORT:-3000}",
    "migrate:dev": "yarn migrate:oldMigrations && yarn prisma:dev",
    "migrate:prod": "yarn migrate:oldMigrations && yarn prisma:deploy",
    "migrate:oldMigrations": "cd ./migrations && yarn migrate && cd ..",
    "migrate:toPrisma": "prisma migrate resolve --applied 20220506182127_initial",
    "prisma:deploy": "prisma migrate deploy && ts-node --compiler-options {\\\"module\\\":\\\"CommonJS\\\"} prisma/seeds/seed.ts --environment=production",
    "prisma:dev": "prisma migrate dev",
    "prisma:reset": "prisma migrate reset",
    "prisma:studio": "prisma studio",
    "initialize": "cd ./flag_initialization && yarn initialize && cd ..",
    "start:test": "APP_ENV=test DATABASE_URL=postgres://postgres:password@localhost:5432 next start",
    "dev:test": "APP_ENV=test DATABASE_URL=postgres://postgres:password@localhost:5432 NODE_OPTIONS='--inspect' next dev",
    "build:test": "APP_ENV=test DATABASE_URL=postgres://postgres:password@localhost:5432 next build",
    "lint": "eslint . ",
    "lint:report": "eslint --output-file eslint_report.json --format json . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --fix .",
    "format": "prettier --config .prettierrc.json --write .",
    "format:check": "prettier --config .prettierrc.json --check .",
    "test": "APP_ENV=test DATABASE_URL=postgres://postgres:password@localhost:5432 jest",
    "test:watch": "DEBUG_PRINT_LIMIT=10000 APP_ENV=test DATABASE_URL=postgres://postgres:password@localhost:5432 jest --watch",
    "cypress": "cypress open",
    "cypress:headless": "cypress run --browser chrome --headless",
    "build-storybook": "build-storybook -s public --no-dll",
    "storybook": "start-storybook -s public -p 6006",
    "postinstall": "[ \"$NODE_ENV\" = production ] && exit 0; husky install"
  },
