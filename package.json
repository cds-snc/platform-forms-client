{
  "name": "gcforms",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev:mem": "yarn migrate && yarn initialize && NODE_OPTIONS='--inspect --max_old_space_size=4096' next dev",
    "dev": "yarn migrate && yarn initialize && NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "yarn migrate && yarn initialize && next start -p ${PORT:-3000}",
    "migrate": "cd ./migrations && yarn migrate && cd ..",
    "initialize": "cd ./flag_initialization && yarn initialize && cd ..",
    "start:test": "CYPRESS=true next start",
    "lint": "eslint . ",
    "lint:report": "eslint --output-file eslint_report.json --format json . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --fix .",
    "format": "prettier --config .prettierrc.json --write .",
    "format:check": "prettier --config .prettierrc.json --check .",
    "test": "jest",
    "cypress": "cypress open",
    "cypress:headless": "cypress run --browser chrome --headless",
    "build-storybook": "build-storybook -s public --no-dll",
    "deploy-storybook": "storybook-to-ghpages",
    "storybook": "start-storybook -s public -p 6006",
    "heroku-postbuild": "cd ./migrations && yarn install && cd ../flag_initialization && yarn install && cd .. &&  yarn build",
    "postinstall": "[ \"$NODE_ENV\" = production ] && exit 0; husky install"
  },
