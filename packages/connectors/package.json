    "prepack": "yarn build",
    "connectors:prepack": "yarn prepack"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.901.0",
    "axios": "^1.12.2",
    "postgres": "^3.4.5"
  },
  "devDependencies": {
    "@types/node": "^24.6.1",
    "tsdown": "^0.15.6",
    "typescript": "^5.9.3"
  },
  "packageManager": "yarn@4.10.3"
}
